{
    "name": "freedom-guard",
    "productName": "Freedom Guard",
    "description": "Freedom Guard +x Freedom Vibe, Freedom Browser, Freedom Link",
    "keywords": [],
    "main": "./main.js",
    "version": "5.0.0",
    "author": "Freedom Guard <FreedomGuard@duck.com>",
    "scripts": {
        "start": "electron .",
        "win": "npx electron-builder --win",
        "linux": "npx electron-builder --linux",
        "build": "npx electron-builder",
        "build:win": "electron-builder --win",
        "build:linux": "electron-builder --linux deb"
    },
    "build": {
        "compression": "maximum",
        "protocols": [
            {
                "name": "freedom guard",
                "schemes": [
                    "freedom-guard"
                ]
            }
        ],
        "extraResources": [
            {
                "from": "assets/",
                "to": "assets/",
                "filter": [
                    "**/*"
                ]
            }
        ],
        "productName": "Freedom Guard",
        "appId": "guard.freedom.com",
        "artifactName": "freedom-guard-${os}.${ext}",
        "asar": true,
        "directories": {
            "output": "dist"
        },
        "win": {
            "icon": "src/assets/icon/ico.png",
            "target": "nsis",
            "requestedExecutionLevel": "requireAdministrator",
            "legalTrademarks": "(c) 2025 freedom-guard.github.io/Freedom",
            "extraResources": [
                {
                    "from": "src/main/cores/win32/",
                    "to": "src/main/cores/win32/",
                    "filter": [
                        "**/*"
                    ]
                }
            ],
            "files": [
                "!src/main/cores/**/*"
            ]
        },
        "mac": {
            "icon": "src/assets/icon/ico.png",
            "target": [
                "dmg",
                "zip"
            ],
            "category": "public.app-category.networking.vpn",
            "extraResources": [
                {
                    "from": "src/main/cores/mac/",
                    "to": "src/main/cores/mac/",
                    "filter": [
                        "**/*"
                    ]
                }
            ],
            "files": [
                "!src/main/cores/**/*"
            ]
        },
        "nsis": {
            "allowToChangeInstallationDirectory": true,
            "oneClick": false,
            "license": "./LICENSE"
        },
        "linux": {
            "icon": "src/assets/icon/ico.png",
            "target": [
                "deb",
                "tar.gz",
                "rpm",
                "AppImage"
            ],
            "category": "VPN",
            "publish": false,
            "extraResources": [
                {
                    "from": "src/main/cores/linux/",
                    "to": "src/main/cores/linux/",
                    "filter": [
                        "**/*"
                    ]
                }
            ],
            "files": [
                "!src/main/cores/**/*"
            ]
        },
        "publish": {
            "provider": "github",
            "publishAutoUpdate": false
        },
        "files": [
            "!assets/**/*",
            "src/**/*",
            "!src/main/cores",
            "!src/plus/apps",
            "!dist/",
            "node_modules/**/*",
            "package.json",
            "main.js",
            "*.js",
            "*.html",
            "*.css",
            "*.png",
            "*.ico",
            "!one.one",
            "*.bat",
            "!freedom-guard.json",
            "!data/**/*"
        ]
    },
    "dependencies": {
        "@aptabase/electron": "^0.3.1",
        "axios": "^1.8.2",
        "bootstrap-icons": "^1.11.3",
        "electron-google-analytics": "^1.0.2",
        "geoip-lite": "^1.4.10",
        "jquery": "^3.7.1",
        "jquery.easing": "^1.4.1",
        "music-metadata": "^10.6.2",
        "music-metadata-browser": "^2.5.11",
        "node-gyp": "^10.1.0",
        "node-notifier": "^10.0.1",
        "winreg": "^1.2.5"
    },
    "devDependencies": {
        "electron": "^31.1.0",
        "fs": "^0.0.1-security"
    },
    "bugs": {
        "url": "https://github.com/Freedom-Guard/Freedom-Guard/issues",
        "email": "fwldom@duck.com"
    }
}